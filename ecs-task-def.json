{
  "family": "rain-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::277546701765:role/ecsTaskExecutionRoleNew",
  "containerDefinitions": [
    {
      "name": "rain-predictor",
      "image": "277546701765.dkr.ecr.us-east-1.amazonaws.com/australia-rain-prediction",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024"
}
